						<property>/position/gear-agl-ft</property>
						<value>2450</value>
					</greater-than>
					<less-than>
						<property>/position/gear-agl-ft-2-sec</property>
						<property>/position/gear-agl-ft</property>
					</less-than>
					<greater-than-equals>
						<property>/systems/electrical/outputs/mk-viii</property>
						<value>25</value>
					</greater-than-equals>
				</and>
			</condition>
			<volume>
				<factor>8</factor>
			</volume>
			<reference-dist>20.0</reference-dist>
			<max-dist>100.0</max-dist>
		</gpws>
