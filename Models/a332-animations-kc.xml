<PropertyList>
	
	<!-- Drogues -->
	
	<animation>
		<type>select</type>
		<object-name>refuelProbeLeft</object-name>
		<object-name>refuelProbeRight</object-name>
		<condition>
			<equals>
				<property>sim/multiplay/generic/float[16]</property>
				<value>1</value>
			</equals>
		</condition>
	</animation>
	
