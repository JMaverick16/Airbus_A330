<PropertyList>
	<name>Rolls Royce Trent772C-60 Engines</name>
	<path>trent772c-60.ac</path>

	<!-- Fans Spin Animation -->

	<animation>
		<name>Left Engine Fan</name>
		<type>spin</type>
		<object-name>fan.cone0</object-name>
		<object-name>fan0</object-name>
		<factor>80</factor>
		<property>engines/engine[3]/n1</property>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>-1.9984</x-m>
			<y-m>-9.3101</y-m>
			<z-m>-2.4685</z-m>
		</center>
	</animation>
	
	<animation>
		<name>Right Engine Fan</name>
		<type>spin</type>
		<object-name>fan.cone1</object-name>
		<object-name>fan1</object-name>
		<factor>80</factor>
		<property>engines/engine[4]/n1</property>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>-1.9984</x-m>
			<y-m>9.3101</y-m>
			<z-m>-2.4685</z-m>
		</center>
	</animation>
	
	<model>
		<name>Engine fire</name>
		<path>Aircraft/Airbus_A330/Models/Effects/enginefire.xml</path>
		<offsets>
			<x-m>3.01294</x-m>
			<y-m>-9.30922</y-m>
			<z-m>-2.46368</z-m>
		</offsets>
		<condition>
			<property>systems/failures/engine-left-fire</property>
		</condition>
	</model>
	
	<model>
		<name>Engine fire</name>
		<path>Aircraft/Airbus_A330/Models/Effects/enginefire.xml</path>
		<offsets>
			<x-m>3.01294</x-m>
			<y-m>9.30922</y-m>
			<z-m>-2.46368</z-m>
		</offsets>
		<condition>
			<property>systems/failures/engine-right-fire</property>
		</condition>
	</model>
	
